{
  "name": "leetcode-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "start-storybook -p 3000",
    "build": "build-storybook",
    "test": "react-scripts test",
    "lint": "eslint src",
    "eject": "react-scripts eject",
    "codecov": "codecov"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "index.js"
    ],
    "coverageThreshold": {
      "./src/**/*.js": {
        "statements": 95,
        "branches": 85,
        "functions": 100,
        "lines": 95
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/builder-webpack5": "^6.4.16",
    "@storybook/manager-webpack5": "^6.4.16",
    "@storybook/node-logger": "^6.4.20",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.16",
    "codecov": "^3.8.3",
    "eslint": "^8.8.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "is-class": "^0.0.9",
    "lint-staged": "^12.3.3",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "slash": "^3.0.0",
    "webpack": "^5.67.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  }
}
