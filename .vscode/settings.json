{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.options": {
    "extensions": [
      ".js",
      ".jsx",
      ".md",
      ".mdx"
    ]
  },
  "javascript.validate.enable": false,
  "eslint.validate": [
    "markdown",
    "mdx",
    "javascript",
    "javascriptreact"
  ],
  "eslint.nodePath": ".yarn/sdks",
  "flow.pathToFlow": "${workspaceFolder}/.yarn/sdks/.bin/flow"
}
